<div class="">
<%= link_to "Logout", logout_path, method: :delete %>
</div>

<div class="dashboard-wrapper">
  <div class="dashboard-content valign-wrapper">
    <%= image_tag(@user.image_url, class: "circle") %>
    <h2 class="valign truncate"><%= @user.name %></h2>
  </div>
  <%= link_to "Find More Perfect Pets", feed_path, class: "find-more-btn" %>
  <div class="saved-pets">
    <h3>Saved Pets</h3>
    <% if @saved.nil? %>
      <p>
        You have no saved pets!
      </p>
    <% else %>
    <div>
      <% @saved.each do |pet| %>
        <div class="pet-conainer">

        </div>
      <% end %>
    </div>
    <% end %>
  </div>
</div>
